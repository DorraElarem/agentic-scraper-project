# Configuration environnement
APP_ENV=development
DEBUG=false

# Base de donnÃ©es
DATABASE_URL=postgresql://postgres:dorra123@db:5432/scraper_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=dorra123
POSTGRES_DB=scraper_db

# Redis et Celery - ðŸ”¥ VALEURS SIMPLES SANS GUILLEMETS
REDIS_URL=redis://redis:6379/0
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/1
CELERY_ACCEPT_CONTENT=json
CELERY_TASK_SERIALIZER=json
CELERY_RESULT_SERIALIZER=json
CELERY_TIMEZONE=UTC
CELERYD_PREFETCH_MULTIPLIER=1

# Ollama
OLLAMA_HOST=http://ollama:11434
OLLAMA_TIMEOUT=60
ENABLE_LLM_ANALYSIS=true
OLLAMA_MODEL="tinyllama:latest" 

# Scraping
DEFAULT_DELAY=2.5
REQUEST_TIMEOUT=30
SCRAPE_MAX_RETRIES=3
MAX_CONTENT_LENGTH_KB=5000
SCRAPE_USER_AGENT=Mozilla/5.0 (compatible; AgenticScraper/1.0)

# Python
PYTHONUNBUFFERED=1
PYTHONPATH=/app